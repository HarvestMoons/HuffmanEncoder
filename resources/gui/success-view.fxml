<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>

<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.huffman.gui.controller.SuccessController"
            stylesheets="@backgroundStyle.css, @buttonStyle.css,@labelStyle.css"
            styleClass="success-background">
    <Label fx:id="summaryLabel" styleClass="success-label" text="压缩/解压缩信息"
           wrapText="true" maxWidth="400" alignment="CENTER"/>
    <Button fx:id="confirmButton" text="我知道了" styleClass="orange-button" onAction="#handleOkButtonClick"/>
</AnchorPane>
